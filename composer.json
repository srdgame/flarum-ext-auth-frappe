{
    "name": "srdgame/flarum-ext-auth-frappe",
    "description": "Allow users to log in with Frappe.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "GPL-3.0+",
    "version": "v0.3.0",
    "authors": [
        {
            "name": "Dirk Chang",
            "email": "dirk@kooiot.com"
        }
    ],
    "support": {
        "issues": "https://github.com/srdgame/flarum-ext-auth-frappe/issues",
        "source": "https://github.com/srdgame/flarum-ext-auth-frappe"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6",
        "league/oauth2-client": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Srdgame\\Auth\\Frappe\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Frappe Login",
            "icon": {
                "name": "frappe",
                "backgroundColor": "#D13E32",
                "color": "#fff"
            }
        }
    }
}
